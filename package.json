{
  "name": "igc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "-------- build, dev ---------------------------------------------------------------": "-",
    "clean:dev": "rimraf .next && yarn dev",
    "dev": "next dev -p 3010 --experimental-https",
    "build": "next build",
    "build:local": "cross-env DATA_BUILD_MODE=local next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "serve": "serve",
    "new:component": "cd .hygen && hygen new component",
    "-------- linting ------------------------------------------------------------------": "-",
    "prettier": "prettier --write",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "check-types": "tsc --noemit",
    "-------- ssl ---------------------------------------------------------------------": "-",
    "create:https": "mkcert create-ca && mkcert create-cert",
    "proxy:sb:https": "npx local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem ",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@storyblok/react": "^3.0.10",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "embla-carousel-autoplay": "^8.3.0",
    "embla-carousel-react": "^8.3.0",
    "fs": "^0.0.2",
    "lucide-react": "^0.447.0",
    "next": "14.2.5",
    "react": "^18",
    "react-cookie": "^4.1.1",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-lazyload": "^3.2.0",
    "react-modal": "^3.16.1",
    "react-social-media-embed": "^2.2.10",
    "serve": "^14.2.0",
    "storyblok-js-client": "^4.5.8",
    "storyblok-react": "^0.1.2",
    "storyblok-rich-text-react-renderer": "^2.8.0",
    "swiper": "^8.4.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@material-tailwind/react": "^2.1.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^9.0.4",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.4",
    "postcss": "^8",
    "semantic-release": "^17.4.2",
    "semver": "^7.3.2",
    "storyblok": "^3.10.5",
    "tailwindcss": "^3.4.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
